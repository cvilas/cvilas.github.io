==============================================================\line \line //========================================================================\line // Transform.t.cpp\line //------------------------------------------------------------------------
\line // \\brief\line // The file Transform.t.cpp is an example program that tests the transform\line // related functions provided in the Math Library for the Robotic Platform.\line //\line 
// The following program demonstrates how to find the values of the\line // components of a vector in a fixed reference frame after the vector \line // undergoes a series of transformations about the frame.\line 
//========================================================================\line \line #include "Transform.hpp"\line \line int main()\line \{\line   ColumnVector<4, double> initialPositionVector;\line   ColumnVector<4, double> finalPositionVector;\line 
  Transform firstRotation;\line   Transform secondRotation;\line   Transform transform;\line   double PI=3.141592654;\line \line //========================================================================\line // The problem statement.\line //============
============================================================\line // The vector first rotates about fixed y axis by 90 degrees, then about \line // the fixed x axis by 90 degrees. The vector then translates along the fixed\line 
// Z axis by 6 units. Requirement is to find the final co-ordinates of the \line // vector w.r.t fixed xyz frame.\line //========================================================================\line \line   // The initial position is [1 0 0];\line 
  initialPositionVector(1) = 1;\line   initialPositionVector(2) = 0;\line   initialPositionVector(3) = 0;\line   initialPositionVector(4) = 1;\line   //position vectors are 4x1, the last element being 1 always.\line   \line 
  // Finding the new co-ordinates.\line   transform = xRotation(PI*90/180) * yRotation(PI*90/180)  * translation(0,0,6);\line   // Angles must be specified in radians.\line   finalPositionVector = transform * initialPositionVector;\line   \line 
  cout << "The co-ordinates of the new position vector in fixed frame: \\n" << finalPositionVector << endl;\line   \line   // Translation between the new vector and the intial position of the vector in the fixed frame.\line 
  cout << "Translation between the new and the initial vector: \\n" << transform.getTranslation() << endl;\line \line   \line   // To get the roll/pitch/yaw angles of the new vector.\line   double roll;\line   double pitch;\line   double yaw;\line 
  transform.getRollPitchYaw(roll, pitch, yaw);\line   cout << "roll angle of the new vector: " << roll*180/PI << " degrees" << endl;\line   cout << "pitch angle of the new vector: " << pitch*180/PI << " degrees" << endl;\line 
  cout << "yaw angle of the new vector: " << yaw*180/PI << " degrees" << endl;\line   \line   // You can get back the initial positionvcetor by the inverse transformation..\line   ColumnVector<4, double> initialPositionVectorAgain;\line 
  initialPositionVectorAgain = inverse(transform) * finalPositionVector;\line   cout << "The co-ordinates of the initial position vector re-calculated by inverse transformation: \\n" << initialPositionVectorAgain << endl;\line 
  cout << "The co-ordinates of the initial position vector originally given: \\n" << initialPositionVector << endl;\line \line   return 0;\line \}\line 
