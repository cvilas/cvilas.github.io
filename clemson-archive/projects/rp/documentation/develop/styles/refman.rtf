//========================================================================\line \line void loadAndCheckNumbers(const char *filename, double *buffer, int numNumbers)\line \{\line   loadNumbers(filename, buffer, numNumbers);\line 
  if (status.isStatusError())\line   \{\line     status.setStatusError()\line       << "Status.t: [loadAndCheckNumbers()] "\line       << "Error loading the numbers." << endl;\line     return;\line   \}\line   \line   for (int i = 0; i < 10; i++)\line   
\{\line     if (buffer[i] > 100)\line     \{\line       status.setStatusError()\line         << "Status.t: [loadAndCheckNumbers()] "\line         << "Number too big (" << buffer[i] << ")" << endl;\line       return;\line     \}\line   \}\line \}\line 
\line \line //========================================================================\line // main\line //========================================================================\line \line int main (int argc, char *argv[])\line \{\line   if (argc != 2)
\line   \{\line     cout << "usage: Status.t <filename>" << endl;\line     return -1;\line   \}\line   \line   double buffer[10];\line   loadAndCheckNumbers(argv[1], buffer, 10);\line   \line   if (status.isStatusError())\line   \{\line 
    cout << "An error occured while loading and checking the numbers: " << endl\line          << status.getMessageText();\line     return -1;\line   \}\line   \line   cout << "Numbers successfully loaded and checked: " << endl;\line 
  for (int i = 0; i < 10; i++)\line     cout << buffer[i] << endl;\line   cout << endl;\line \}\line \line 
